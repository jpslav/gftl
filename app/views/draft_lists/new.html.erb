<h1>New draft_list</h1>

<%= form_for(@draft_list) do |f| %>
  <%= render "shared/error_messages", :target => @draft_list %> 

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :notes %><br />
    <%= f.text_area :notes, :rows => 4 %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', draft_lists_path %>